%h3  
  .text-center
    News Feed
    .badge.badge-primary
      = @activities.count

%ul.list-group
  - @activities.each do |activity|
    %li.list-group-item
      = activity.id
      = activity.trackable_type
      = activity.trackable_id
      = activity.key
      = activity.parameters
      = activity.owner